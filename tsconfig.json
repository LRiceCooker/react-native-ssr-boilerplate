{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@my/app/*": ["./packages/app/*"],
      "@my/ui/*": ["./packages/ui/*"],
      "@my/design/*": ["./packages/design/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["**/node_modules", "**/dist", "**/types", "apps/next/out", "apps/next/.next", "apps/next/.tamagui"]
}
